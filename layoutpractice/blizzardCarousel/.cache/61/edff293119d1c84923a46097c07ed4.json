  @next=\"nextHandler\"\n//             @prev=\"prevHandler\"\n//             ></carousel-control>\n//             `\n\nconst carousel = {\n    props: {\n        list: {\n            type: Array,\n            default: []\n        }\n    },\n    data() {\n        return {\n            active: 0,\n            total: this.list.length,\n            direct: 0,\n            isHover: false\n        }\n    },\n    components: {\n        carouselItem,\n        carouselControl\n    },\n    template: \n    `\n    <div class=\"carousel\">\n        <div class=\"carousel-container\">\n            <transition :name=\"trans\">\n                <carousel-item :src=\"list[active].src\" :key=\"list[active].id\"></carousel-item>\n            </transition>  \n            <carousel-control\n            :total=\"total\"\n            :active=\"active\"\n            @click=\"changeItem\"\n            @next=\"nextHandler\"\n            @prev=\"prevHandler\"\n            ></carousel-control>\n        </div>\n    </div>\n    `,\n    computed: {\n        trans() { return this.direct ? 'move-left' : 'move-right' }\n    },\n    methods: {\n        mouseHover() {this.isHover = true },\n        mouseLeave() { this.isHover = false },\n        changeItem(val) { this.active = Math.abs(val) % this.total },\n        nextHandler() {\n            this.direct = 1\n            this.changeItem(this.active + 1)\n        },\n        prevHandler() {\n            this.direct = 0\n            this.changeItem(this.active - 1)\n        },\n        go() { this.isHover ? '' : this.nextHandler() }\n    }\n}\n\nexport default carousel\n"},"lineCount":null}},"error":null,"hash":"6637b3b4493cee7f84162a6edd259346","cacheData":{"env":{}}}